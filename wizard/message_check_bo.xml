<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- message.check.bo.wizard form view -->
    <record id="message_check_bo_wizard_view_form" model="ir.ui.view">
        <field name="name">Warning Message Booking Order</field>
        <field name="model">message.check.bo.wizard</field>
        <field name="arch" type="xml">
            <form string="Check Message">
                <sheet>
                    <field name="message" attrs="{'readonly': True}" />
                    <footer>
                        <button string="Close" class="btn-secondary" special="cancel" />
                    </footer>
                </sheet>
            </form>
        </field>
    </record>

</odoo>